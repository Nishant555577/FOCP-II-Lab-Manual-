#include <iostream>
#include<string>
using namespace std;
int main() {
	
	string name;
	cin>>name;
int x=0,y=0;
	for(int i=0;i<name.length();i++){
if(name[i]=='z'){
x++;
}else{y++;
	}
	}
	if(2*x==y){
		cout<<"Yes";
	}else{
		cout<<"No";
	}
	return 0;
	}
  class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        for(int i=0;i<nums.size();i++){
            for(int j=i+1;j<nums.size();j++){
                if(nums[i]+nums[j]==target){
                    return{i,j};
                }
            }
        }
        return {};
    }
};
