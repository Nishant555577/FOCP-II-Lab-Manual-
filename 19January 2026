#include <iostream>
using namespace std;
int main(){
    int a,b,c;
    a=10;
    b=7;
    a=b;
    b=a;
    cout<<"the value of first numbe is:"<<a;
    cout<<"the vlue of : "<<b;
    return 0;
}
