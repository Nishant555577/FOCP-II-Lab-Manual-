#include <iostream>
#include<string>
using namespace std;
int main() {
	int N;
	string S;
cin>>N;
cin>>S;
	cout<<N*2<<endl;
	cout<<S<<endl;
	return 0;

}
#include <iostream>
#include<string>
using namespace std;
int main() {
	
	string name;
	cin>>name;
int x=0,y=0;
	for(int i=0;i<name.length();i++){
if(name[i]=='z'){
x++;
}else{y++;
	}
	}
	if(2*x==y){
		cout<<"Yes";
	}else{
		cout<<"No";
	}
	return 0;
	}





  
